\NeedsTeXFormat{LaTeX2e}
\ProvidesPackage{beamerouterthemeTUDtitlepage}

\makeatletter

  \setbeamertemplate{title page}{%
    % \beamersetleftmargin{13.86mm}
    % \beamersetrightmargin{9.58mm}
      \hspace*{8.86mm}\begin{minipage}{\dimexpr\textwidth-13.44mm\relax}%
        \color{white}%
        \vfill%
        \usebeamerfont*{title}%
        \vskip2ex plus1ex minus1ex%
        \bigskip
        \MakeUppercase{\inserttitle}%
        \vfill%
        \ifx\insertsubtitle\empty%
        \else%
          \usebeamerfont*{subtitle}%
          \medskip
          \insertsubtitle%
          \vfill%
        \fi%
        \normalfont\Large%
        \bigskip
        \insertauthor%
        \vfill%
      \end{minipage}%
        \vfill%
    %   \usebeamerfont{title}\inserttitle\par
    %   \usebeamerfont{subtitle}\usebeamercolor[fg]{subtitle}\insertsubtitle\par
    %   \bigskip
    %   \usebeamerfont{author}\insertauthor\par
    %   \usebeamerfont{institute}\insertinstitute\par
    %   \usebeamerfont{date}\insertdate\par
    % %   \usebeamercolor[fg]{titlegraphic}\inserttitlegraphic
  }

  \def\maketitle{{
    \setbeamercolor{normal text}{bg=darkblue}%
    \setbeamertemplate{headline}{%
        \vskip6.15mm%
        \color{white}%
        \setlength{\arrayrulewidth}{0.3pt}%
        \begin{tabular*}{\paperwidth}[b]{l@{\extracolsep\fill}}%
          % TUD-Logos
          \hspace*{3.0mm}%
          \color{white}%
          \raisebox{1.1mm}{\includegraphics[height=7.81mm]{\logo@front}}%
          \hspace*{2.5cm}%
          %\includegraphics[height=9.81mm]{cfaed}%          
          \hspace*{10mm}%
          \hspace*{2.5cm}%
          %\raisebox{1.1mm}{\includegraphics[height=7.81mm]{\conceptlogo@front}}%
          \\[1.2mm]\hline%
          \hspace*{11.76mm}%
          \rule[-0.8mm]{0pt}{2.47mm}%
          \def\@@dummyComma{}%
          \rule{0pt}{5.8pt}%
          \textbf{\@einrichtung }\ifx\@einrichtung\@empty\else\def\@@dummyComma{ }\fi%
          \ifx\@fachrichtung\@empty%
          \else%
            \@@dummyComma\@fachrichtung%
            \ifx\@institut\@empty\else\def\@@dummyComma{, }\fi%
            \ifx\@professur\@empty\else\def\@@dummyComma{, }\fi%
          \fi%
          \ifx\@institut\@empty\else\@@dummyComma\@institut%
            \ifx\@professur\@empty\else\def\@@dummyComma{, }\fi%
          \fi%
          \ifx\@professur\@empty\else\@@dummyComma\fi%
          \@professur\\[-1pt]\hline%
        \end{tabular*}\hspace{-\paperwidth}%
    }%
    \setbeamertemplate{footline}{%
        \parbox[t][9.63mm]{\paperwidth}{%
            \vspace*{-5.63mm}%
            \rule{13.86mm}{0pt}%
            \normalsize%
            \color{white}%
%             \insertdatecity%
%             \ifx\insertdatecity%
%               \empty%
%             \else%
%               \ifx\insertdate%
%                 \empty%
%               \else%
%                 , %
%               \fi%
%             \fi%
            \insertdate%
        }
    }%
  \ifbeamer@inframe\titlepage\else\frame{\titlepage}\fi%
  \addtocounter{framenumber}{-1}%
  }}
  
\makeatother
